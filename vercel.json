{
    "builds":[
        {
            "src":"Mooring/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"50mb",
                "runtime":"python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "media"
            }
        }
    ],
    "routes":[
        {
            "src": "/media/(.*)",
            "dest": "/media/static/$1"
        },
        {
            "src":"/(.*)",
            "dest": "Mooring/wsgi.py"
        }
    ]
}